{
  "name": "sahil-portfolio",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://sahilthecoder.github.io/Sahil-Portfolio",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sahilthecoder/Sahil-Portfolio.git"
  },
  "bugs": {
    "url": "https://github.com/Sahilthecoder/Sahil-Portfolio/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run generate-favicons && vite build --base=/Sahil-Portfolio",
    "preview": "vite preview --base=/Sahil-Portfolio",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist -t true -m 'Deploy to GitHub Pages [skip ci]'",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .js,.jsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css}\"",
    "start": "vite --host",
    "serve": "vite preview --port 4173",
    "preview:prod": "vite preview --port 4173 --host",
    "analyze": "cross-env VITE_BUNDLE_ANALYZE=true vite build",
    "audit:performance": "lighthouse http://localhost:3000 --view --preset=desktop",
    "audit:accessibility": "pa11y http://localhost:3000 --standard WCAG2AA",
    "generate-favicons": "node scripts/create-favicons.js",
    "postinstall": "npm run generate-favicons",
    "audit:all": "npm run audit:performance && npm run audit:accessibility"
  },
  "dependencies": {
    "@sentry/react": "^7.74.0",
    "@sentry/replay": "^7.74.0",
    "canvas": "^3.1.2",
    "framer-motion": "^10.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-parallax-tilt": "^1.7.299",
    "react-router-dom": "^6.22.3",
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1",
    "swiper": "^11.2.10"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@langchain/core": "^0.3.62",
    "@langchain/openai": "^0.5.18",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/node": "^24.0.9",
    "@types/react": "^18.3.23",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.17",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.7",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "fs-extra": "^11.3.0",
    "gh-pages": "^5.0.0",
    "lighthouse": "^12.7.0",
    "pa11y": "^9.0.0",
    "postcss": "^8.4.35",
    "postcss-import": "^16.1.1",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^10.2.4",
    "prettier": "^3.1.1",
    "prop-types": "^15.8.1",
    "sass": "^1.89.2",
    "sass-embedded": "^1.89.2",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.17",
    "terser": "^5.27.0",
    "vite": "^6.1.6",
    "vitest": "^3.2.4"
  }
}
