{
  "name": "sahil-portfolio",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://sahilthecoder.github.io/Sahil-Portfolio",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sahilthecoder/Sahil-Portfolio.git"
  },
  "bugs": {
    "url": "https://github.com/Sahilthecoder/Sahil-Portfolio/issues"
  },
  "scripts": {
    "dev": "vite --host --port 3001",
    "build": "vite build --mode production && npm run copy-assets && npm run copy-favicons",
    "copy-assets": "node scripts/copy-assets.js",
    "copy-favicons": "node scripts/copy-favicons.js",
    "preview": "vite preview --port 4173 --host",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "predeploy": "npm run build && npm run generate-sitemap",
    "deploy": "gh-pages -d dist --dotfiles -t true -m 'Deploy to GitHub Pages [skip ci]' --repo https://github.com/Sahilthecoder/Sahil-Portfolio.git",
    "deploy:ci": "cross-env CI=true npm run deploy",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .js,.jsx --fix",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "start": "vite --host --port 3000",
    "serve": "vite preview --port 3000 --host",
    "preview:prod": "vite preview --port 3000 --host",
    "analyze": "cross-env VITE_BUNDLE_ANALYZE=true vite build",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "audit:performance": "lighthouse http://localhost:3000 --view --preset=desktop --output-path=./lighthouse-report.html",
    "audit:accessibility": "pa11y http://localhost:3000 --standard WCAG2AA --reporter html > pa11y-report.html",
    "audit:all": "npm run audit:performance && npm run audit:accessibility",
    "postinstall": "node scripts/postinstall.js",
    "generate-favicons": "node scripts/generate-favicons.js"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@heroicons/react": "^2.2.0",
    "@react-spring/web": "^10.0.1",
    "@sentry/integrations": "^7.120.3",
