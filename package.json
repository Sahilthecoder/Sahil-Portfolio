{
  "name": "sahil-portfolio",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://sahilthecoder.github.io/Sahil-Portfolio/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sahilthecoder/Sahil-Portfolio.git"
  },
  "bugs": {
    "url": "https://github.com/Sahilthecoder/Sahil-Portfolio/issues"
  },
  "scripts": {
    "optimize-images": "node scripts/optimize-images.js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist -t true -m 'Deploy to GitHub Pages [skip ci]'",
    "lint": "eslint src --ext .js,.jsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css}\"",
    "start": "vite --host",
    "serve": "vite preview --port 4173",
    "preview:prod": "vite preview --port 4173 --host",
    "analyze": "cross-env VITE_BUNDLE_ANALYZE=true vite build",
    "audit:performance": "lighthouse http://localhost:3000 --view --preset=desktop",
    "audit:accessibility": "pa11y http://localhost:3000 --standard WCAG2AA"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@sendgrid/mail": "^8.1.5",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/line-clamp": "^0.4.4",
    "clsx": "^2.1.1",
    "framer-motion": "^10.12.16",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^4.12.0",
    "react-parallax-tilt": "^1.7.300",
    "react-responsive": "^10.0.1",
    "react-router-dom": "^6.14.2",
    "react-type-animation": "^3.2.0",
    "swiper": "^11.2.10",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.19.7",
    "@types/react": "^18.3.23",
    "@types/styled-jsx": "^2.2.9",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.7",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "lighthouse": "^12.7.1",
    "pa11y": "^9.0.0",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-nesting": "^13.0.2",
    "prettier": "^3.0.2",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.3.3",
    "terser": "^5.43.1",
    "vite": "^7.0.3"
  }
