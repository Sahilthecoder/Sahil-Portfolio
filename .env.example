# OpenAI API Key (required for AI responses)
VITE_OPENAI_API_KEY=your_openai_api_key
VITE_SENTRY_DSN=your_sentry_dsn
VITE_PLAUSIBLE_DOMAIN=your_domain
VITE_CLOUDINARY_CLOUD_NAME=your_cloudinary_name
VITE_PLAUSIBLE_API_HOST=https://plausible.io_here

# Pinecone API Key (optional, for vector search)
REACT_APP_PINECONE_API_KEY=your_pinecone_api_key_here
REACT_APP_PINECONE_ENVIRONMENT=your_pinecone_environment
REACT_APP_PINECONE_INDEX=your_pinecone_index_name

# Enable/disable features
REACT_APP_ENABLE_VOICE_INPUT=true
REACT_APP_ENABLE_FILE_UPLOAD=true
