<?xml version="1.0" encoding="utf-8"?>
<browserconfig>
  <msapplication>
    <tile>
      <square70x70logo src="/mstile-70x70.png"/>
      <square150x150logo src="/mstile-150x150.png"/>
      <square310x310logo src="/mstile-310x310.png"/>
      <TileColor>#2563eb</TileColor>
      <TileImage src="/mstile-144x144.png" />
    </tile>
    <notification>
      <polling-uri src="/push/notifications" />
      <polling-uri2 src="/push/notifications2" />
      <polling-uri3 src="/push/notifications3" />
      <polling-uri4 src="/push/notifications4" />
      <polling-uri5 src="/push/notifications5" />
      <frequency>
        <minimum>30</minimum>
        <desired>60</desired>
      </frequency>
      <cycle>1</cycle>
    </notification>
  </msapplication>
</browserconfig>
