{
  "name": "sahil-portfolio",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://sahilthecoder.github.io/Sahil-Portfolio/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sahilthecoder/Sahil-Portfolio.git"
  },
  "bugs": {
    "url": "https://github.com/Sahilthecoder/Sahil-Portfolio/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist -t true -m 'Deploy to GitHub Pages [skip ci]'",
    "lint": "eslint src --ext .js,.jsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css}\"",
    "start": "vite --host",
    "serve": "vite preview --port 4173",
    "preview:prod": "vite preview --port 4173 --host",
    "analyze": "cross-env VITE_BUNDLE_ANALYZE=true vite build",
    "audit:performance": "lighthouse http://localhost:3000 --view --preset=desktop",
    "audit:accessibility": "pa11y http://localhost:3000 --standard WCAG2AA",
    "copy-favicons": "node scripts/copy-favicons.js",
    "copy-favicons-root": "node scripts/copy-favicons-root.js",
    "prebuild": "npm run copy-favicons"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "framer-motion": "^10.12.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^4.10.1",
    "react-intersection-observer": "^9.5.2",
    "react-parallax-tilt": "^1.7.300",
    "react-router-dom": "^6.14.2",
    "react-syntax-highlighter": "^5.8.0",
    "react-type-animation": "^3.2.0",
    "swiper": "^11.2.10"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@emotion/babel-plugin": "^11.13.5",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.5.3",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.7",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lighthouse": "^12.7.1",
    "pa11y": "^9.0.0",
    "postcss": "^8.4.28",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.1",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "sharp": "^0.34.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^7.0.3",
    "vite-plugin-pwa": "^0.16.4",
    "web-vitals": "^3.4.0"
  }
}
